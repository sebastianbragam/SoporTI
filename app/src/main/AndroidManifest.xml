<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.saltapor.soporti">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SoporTI">
        <activity
            android:name=".EditProfileActivity"
            android:exported="false"
            android:label="@string/edit_profile" />
        <activity
            android:name=".UserFinishActivity"
            android:exported="false"
            android:label="@string/finish_ticket" />
        <activity
            android:name=".FilterActivity"
            android:exported="false"
            android:label="@string/filter" />
        <activity
            android:name=".DocumentationActivity"
            android:exported="false"
            android:label="@string/documentation" />
        <activity
            android:name=".EditUserActivity"
            android:exported="false"
            android:label="@string/edit_user_role" />
        <activity
            android:name=".UsersActivity"
            android:exported="false"
            android:label="@string/rol_by_user" />
        <activity
            android:name=".FilesFinishedActivity"
            android:exported="false"
            android:label="@string/files" />
        <activity
            android:name=".NewFileActivity"
            android:exported="false"
            android:label="@string/new_file" />
        <activity
            android:name=".FilesActivity"
            android:exported="false"
            android:label="@string/files" />
        <activity
            android:name=".ReportCategoryActivity"
            android:exported="false"
            android:label="@string/report_by_type_and_category" />
        <activity
            android:name=".ReportActivity"
            android:exported="false"
            android:label="@string/report_by_type" />
        <activity
            android:name=".ViewFinishedTicketActivity"
            android:exported="false"
            android:label="@string/finished_ticket" />
        <activity
            android:name=".FinishedTicketsActivity"
            android:exported="false"
            android:label="@string/finished_tickets" />
        <activity
            android:name=".SupportFinishActivity"
            android:exported="false"
            android:label="@string/conclude_ticket" />
        <activity
            android:name=".SupportReplyActivity"
            android:exported="false"
            android:label="@string/reply_ticket" />
        <activity
            android:name=".AssignTicketActivity"
            android:label="@string/assign_ticket"
            android:parentActivityName=".AdminTicketsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".AdminTicketsActivity" />
        </activity>
        <activity
            android:name=".SupportTicketsActivity"
            android:exported="false"
            android:label="@string/soporti_support" />
        <activity
            android:name=".UserTicketsActivity"
            android:exported="false"
            android:label="@string/soporti_user" />
        <activity
            android:name=".UserCheckActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdminTicketsActivity"
            android:exported="false" />
        <activity
            android:name=".UserReplyActivity"
            android:label="@string/reply_ticket"
            android:parentActivityName=".ViewTicketActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ViewTicketActivity" />
        </activity>
        <activity
            android:name=".ViewTicketActivity"
            android:label="@string/ticket"
            android:parentActivityName=".UserTicketsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserTicketsActivity" />
        </activity>
        <activity
            android:name=".EditCategoryActivity"
            android:exported="false"
            android:label="@string/edit_category" />
        <activity
            android:name=".CategoriesActivity"
            android:label="@string/categories"
            android:parentActivityName=".UserTicketsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserTicketsActivity" />
        </activity>
        <activity
            android:name=".NewCategoryActivity"
            android:label="@string/new_category"
            android:parentActivityName=".CategoriesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CategoriesActivity" />
        </activity>
        <activity
            android:name=".NewTicketActivity"
            android:label="@string/new_ticket"
            android:parentActivityName=".UserTicketsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserTicketsActivity" />
        </activity>
        <activity
            android:name=".NewUserActivity"
            android:label="@string/new_user"
            android:parentActivityName=".UserTicketsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserTicketsActivity" />
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/login" />
    </application>

</manifest>